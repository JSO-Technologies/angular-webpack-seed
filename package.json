{
  "name": "talend-frontend-seed",
  "version": "1.0.0",
  "description": "Seed frontend project for Talend",
  "main": "build/app.js",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:dist && npm run clean:coverage",
      "clean:build": "rimraf ./build",
      "clean:dist": "rimraf ./dist",
      "clean:coverage": "rimraf ./coverage",

    "build": "npm run build:dev",
      "prebuild:dev": "npm run clean:build",
      "build:dev": "webpack --config config/webpack.config.dev.js --progress --profile --color --display-error-details --display-cached",
      "prebuild:dist": "npm run clean:dist",
      "build:dist": "webpack --config config/webpack.config.prod.js --progress --profile --color --display-error-details --display-cached",

    "serve": "npm run serve:dev",
      "preserve:dev": "npm run clean:build",
      "serve:dev": "webpack-dev-server --config config/webpack.config.dev.js --progress --colors --open",
      "preserve:dist": "npm run clean:dist",
      "serve:dist": "webpack-dev-server --config config/webpack.config.prod.js --progress --colors --open --compress",

    "test": "karma start",
      "test:auto": "karma start --auto",

    "lint": "npm run lint:es",
      "lint:es": "eslint --config .eslintrc src/app/**/*.js",
      "lint:sass": "sass-lint -c .sass-lint.yml 'src/app/**/*.scss' -v -q",

    "start": "npm run serve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "bourbon": "^4.2.7"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "isparta-loader": "^2.0.0",
    "jasmine": "^2.4.1",
    "jquery": "^2.2.3",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.12.0",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.2.0",
    "sasslint-webpack-plugin": "^1.0.3",
    "stripcomment-loader": "^0.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.7",
    "webpack-dev-server": "^1.14.1"
  }
}
