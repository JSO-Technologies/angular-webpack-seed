{
  "name": "talend-frontend-seed",
  "version": "1.0.0",
  "description": "Seed frontend project for Talend",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:dist",
      "clean:build": "rimraf ./build",
      "clean:dist": "rimraf ./dist",

    "build": "npm run build:dev",
      "prebuild:dev": "npm run clean:build",
      "build:dev": "webpack --config webpack.config.dev.js --progress --profile --colors --display-error-details --display-cached",
      "prebuild:prod": "npm run clean:dist",
      "build:prod": "webpack --config webpack.config.prod.js --progress --profile --colors --display-error-details --display-cached",

    "start": "webpack-dev-server --config webpack.config.dev.js --progress --colors --hot --content-base build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.5",
    "bourbon": "^4.2.7"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.1",
    "eslint-plugin-import": "^1.7.0",
    "html-webpack-plugin": "^2.16.1",
    "isparta-loader": "^2.0.0",
    "lodash": "^4.12.0",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.7.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stripcomment-loader": "^0.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
